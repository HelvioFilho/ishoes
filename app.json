{
  "expo": {
    "name": "iShoes",
    "slug": "iShoes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/main/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./src/assets/main/splash.png",
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "android": {
      "package": "com.hsvf.ishoes"
    },
    "ios": {
      "bundleIdentifier": "com.hsvf.ishoes"
    },
    "description": "iShoes is an online shoes sales app.",
    "githubUrl": "https://github.com/HelvioFilho/ishoes",
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development",
          "smallIcons": ["./src/assets/main/notification.png"]
        }
      ]
    ]
  }
}